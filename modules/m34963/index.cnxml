                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mn>8</m:mn>
                            </m:mrow>
                            <m:mtext>.</m:mtext>
                            <m:mrow>
                              <m:mtext>315274</m:mtext>
                              <m:mo stretchy="false">=</m:mo>
                              <m:mtext>83</m:mtext>
                            </m:mrow>
                            <m:mtext>.</m:mtext>
