                        </m:mrow>
                      </m:mstyle>
                      <m:mrow/>
                    </m:mrow>
                    <m:annotation encoding="StarMath 5.0"> size 12{"10" cdot 8 "." "315274"="83" "." "15274"} {}</m:annotation>
                  </m:semantics>
                </m:math>
              </entry>
              <entry>1</entry>
