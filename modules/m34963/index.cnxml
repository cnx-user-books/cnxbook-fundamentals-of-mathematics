                        <m:mrow>
                          <m:mrow>
                            <m:mrow>
                              <m:mtext>100</m:mtext>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mn>8</m:mn>
                            </m:mrow>
                            <m:mtext>.</m:mtext>
                            <m:mrow>
                              <m:mtext>315274</m:mtext>
