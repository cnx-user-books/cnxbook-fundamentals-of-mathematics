      <exercise id="id1165021114967">
        <problem id="oo-id1165024940199">
          <para id="id1165020815134">
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>50</m:mtext><m:mo stretchy="false">⋅</m:mo><m:mtext>63</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"50" cdot "63"} {}</m:annotation></m:semantics></m:math></para>
        </problem>
<solution id="eip-id1169832368809"><para id="eip-id1169825797307"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>50</m:mtext><m:mrow><m:mfenced open="(" close=")"><m:mrow><m:mtext>60</m:mtext><m:mo stretchy="false">+</m:mo><m:mn>3</m:mn></m:mrow></m:mfenced><m:mo stretchy="false">=</m:mo><m:mn>3,</m:mn></m:mrow><m:mrow><m:mrow><m:mtext>000</m:mtext><m:mo stretchy="false">+</m:mo><m:mtext>150</m:mtext></m:mrow><m:mo stretchy="false">=</m:mo><m:mn>3,</m:mn></m:mrow><m:mtext>150</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"50" left ("60"+3 right )=3,"000"+"150"=3,"150"} {}</m:annotation></m:semantics></m:math></para></solution>
      </exercise>
