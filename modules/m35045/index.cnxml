                            <m:mrow>
                              <m:mrow>
                                <m:mn>1</m:mn>
                                <m:mo stretchy="false">⋅</m:mo>
                                <m:mi>a</m:mi>
                              </m:mrow>
                              <m:mo stretchy="false">=</m:mo>
                              <m:mtext>14</m:mtext>
                            </m:mrow>
                          </m:mrow>
                        </m:mstyle>
                        <m:mrow/>
                      </m:mrow>
</m:mtd>                    
</m:mtr>
                    <m:mtr>
<m:mtd columnalign="left">        
              <m:mrow>
                        <m:mstyle fontsize="12pt">
                          <m:mrow>
                            <m:mrow>
                              <m:mi>a</m:mi>
                              <m:mo stretchy="false">=</m:mo>
                              <m:mtext>14</m:mtext>
                            </m:mrow>
                          </m:mrow>
                        </m:mstyle>
                        <m:mrow/>
                      </m:mrow>
</m:mtd>                   
 </m:mtr>
                  </m:mtable>
                  <m:mrow/>
