          <para id="id7028231">0</para>
        </problem>
<solution id="eip-id1164920636234"><para id="eip-id2242704"><emphasis effect="italics">W, Z</emphasis></para></solution>
      </exercise>
      <exercise id="id7026224">
        <problem id="oo-id6614084">
          <para id="id7020460">1</para>
        </problem>
      </exercise>
      <exercise id="id7026216">
        <problem id="oo-id5887740">
          <para id="id7020691">-3</para>
        </problem>
<solution id="eip-id1164930293296"><para id="eip-id1164940947165"><emphasis effect="italics">Z</emphasis></para></solution>
      </exercise>
      <exercise id="id7026178">
        <problem id="oo-id6253774">
