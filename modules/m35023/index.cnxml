<m:mtd/>
<m:mtd><m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">        
                <m:mtext>270 cu in.</m:mtext>
</m:mtd>                        
   </m:mtr>
                    <m:mtr>
<m:mtd/>
<m:mtd><m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">    
<m:msup>   
                <m:mtext>270 in.</m:mtext>
<m:mn>3</m:mn>
</m:msup>        
</m:mtd>         
