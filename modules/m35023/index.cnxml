                            </m:mfenced>
                            <m:mo stretchy="false">⋅</m:mo>
                            <m:msup>
                              <m:mfenced open="(" close=")">
                                  <m:mtext>4.9 ft</m:mtext>
                              </m:mfenced>
                                  <m:mn>2</m:mn>
                            </m:msup>
                          <m:mo stretchy="false">⋅</m:mo>
