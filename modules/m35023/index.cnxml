                                <m:mn>3</m:mn>
                                  </m:msup>
</m:mtd>                     
     </m:mtr>
                    <m:mtr>
<m:mtd/>
<m:mtd>
<m:mo>≈</m:mo>
</m:mtd>
<m:mtd columnalign="left">        
                    <m:mfenced open="(" close=")">
                              <m:mfrac>
