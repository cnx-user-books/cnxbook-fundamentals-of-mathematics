<m:mtd>
<m:mo>≈</m:mo>
</m:mtd>
<m:mtd columnalign="left">        
                    <m:mfenced open="(" close=")">
                              <m:mfrac>
                                <m:mn>4</m:mn>
                                <m:mn>3</m:mn>
                              </m:mfrac>
                            </m:mfenced>
                            <m:mo stretchy="false">⋅</m:mo>
                            <m:mfenced open="(" close=")">
                                <m:mn>3.14</m:mn>
                                    </m:mfenced>
                          <m:mo stretchy="false">⋅</m:mo>
                          <m:msup>
                            <m:mfenced open="(" close=")">
