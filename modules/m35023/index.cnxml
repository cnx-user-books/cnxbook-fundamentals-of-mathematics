                            </m:mfenced>
                            <m:mo stretchy="false">⋅</m:mo>
                            <m:mfenced open="(" close=")">
<m:mn>3.14</m:mn>                        
    </m:mfenced>
                          <m:mo stretchy="false">⋅</m:mo>
                          <m:mfenced open="(" close=")">
                              <m:mtext>216 cu cm</m:mtext>
                          </m:mfenced>
</m:mtd>         
           </m:mtr>
<m:mtr>
<m:mtd/>
<m:mtd>
<m:mo>≈</m:mo>
</m:mtd>
<m:mtd columnalign="left">
<m:mtext>904.32 cu cm</m:mtext>
</m:mtd>
</m:mtr>
