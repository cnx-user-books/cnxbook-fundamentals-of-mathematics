          </problem>
        <solution id="eip-id1165614947150"><para id="eip-id1165615953053"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mn>1</m:mn><m:mfrac><m:mn>3</m:mn><m:mtext>11</m:mtext></m:mfrac></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{1 {  {3}  over  {"11"} } } {}</m:annotation></m:semantics></m:math></para></solution></exercise>
        <exercise id="id1169195932296"><problem id="oo-id1169196323588">
            <para id="id1169196341684"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mfrac><m:mtext>31</m:mtext><m:mtext>13</m:mtext></m:mfrac></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ {  {"31"}  over  {"13"} } } {}</m:annotation></m:semantics></m:math></para>
          </problem>
        <solution id="eip-id1167553346279"><para id="eip-id8892143"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mn>2</m:mn><m:mfrac><m:mn>5</m:mn><m:mtext>13</m:mtext></m:mfrac></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{2 {  {5}  over  {"13"} } } {}</m:annotation></m:semantics></m:math></para></solution></exercise>
        <exercise id="id1169195807144"><problem id="oo-id1169195978283">
            <para id="id1169196771103"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mfrac><m:mtext>79</m:mtext><m:mn>4</m:mn></m:mfrac></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ {  {"79"}  over  {4} } } {}</m:annotation></m:semantics></m:math></para>
          </problem>
        <solution id="eip-id2417044"><para id="eip-id1167554353899"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>19</m:mtext><m:mfrac><m:mn>3</m:mn><m:mn>4</m:mn></m:mfrac></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"19" {  {3}  over  {4} } } {}</m:annotation></m:semantics></m:math></para></solution></exercise>
        <exercise id="id1169201809791"><problem id="oo-id1169209207687">
            <para id="id1169203880736"> 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mfrac><m:mtext>496</m:mtext><m:mn>8</m:mn></m:mfrac></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ {  {"496"}  over  {8} } } {}</m:annotation></m:semantics></m:math> </para>
          </problem>
        <solution id="eip-id1167434033319"><para id="eip-id1167430766802">62</para></solution></exercise>
