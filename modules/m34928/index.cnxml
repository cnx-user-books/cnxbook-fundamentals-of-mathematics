                </m:mstyle>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ {  { { { {2}} { {2}}}  cSup { size 8{2} } }  over  { { { {5}}}  cSub { size 8{1} } } }  cdot  {  { { { {1}} { {0}}}  cSup { size 8{2} } }  over  { { { {1}} { {1}}}  cSub { size 8{1} } } } = {  {2 cdot 2}  over  {1 cdot 1} } = {  {4}  over  {1} } =4} {}</m:annotation>
            </m:semantics>
