                                  </m:mrow>
                                </m:mstyle>
                              </m:munder>
                            </m:mfrac>
                            <m:mo stretchy="false">⋅</m:mo>
                            <m:mfrac>
                              <m:mover>
                                <m:mrow>
                                <m:menclose notation="updiagonalstrike">  <m:mn>10</m:mn></m:menclose>
                                  
                                </m:mrow>
                                <m:mstyle fontsize="8pt">
                                  <m:mrow>
                                    <m:mn>2</m:mn>
                                  </m:mrow>
                                </m:mstyle>
                              </m:mover>
                              <m:munder>
                                <m:mrow>
                                 <m:menclose notation="updiagonalstrike"> <m:mn>11</m:mn></m:menclose>
                                  
                                </m:mrow>
                                <m:mstyle fontsize="8pt">
                                  <m:mrow>
                                    <m:mn>1</m:mn>
                                  </m:mrow>
                                </m:mstyle>
                              </m:munder>
                            </m:mfrac>
                          </m:mrow>
                          <m:mo stretchy="false">=</m:mo>
                          <m:mfrac>
                            <m:mrow>
                              <m:mn>2</m:mn>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mn>2</m:mn>
                            </m:mrow>
                            <m:mrow>
                              <m:mn>1</m:mn>
