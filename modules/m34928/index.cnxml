                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ {  { { { {5}} { {5}}}  cSup { size 8{"11"} } }  over  { { { {6}}}  cSub { size 8{2} } } }  cdot  {  { { { {6}} { {3}}}  cSup { size 8{"21"} } }  over  { { { {5}}}  cSub { size 8{1} } } } = {  {"11" cdot "21"}  over  {2 cdot 1} } = {  {"231"}  over  {2} } ="115" {  {1}  over  {2} } } {}</m:annotation>
            </m:semantics>
