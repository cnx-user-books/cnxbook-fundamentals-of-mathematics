                    <m:mtext>.</m:mtext>
                    
                      <m:mtext>22</m:mtext>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mn>4</m:mn>
                   
                      <m:mfrac>
                        <m:mtext>37</m:mtext>
                        <m:mtext>100</m:mtext>
                      </m:mfrac>
