<m:mtd>
<m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">
<m:mfrac>
<m:mrow><m:mn>18</m:mn><m:mo>+</m:mo><m:mn>2</m:mn></m:mrow>
