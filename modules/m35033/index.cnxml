<m:mrow><m:mo>-</m:mo><m:mo>(</m:mo><m:mn>4</m:mn><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>)</m:mo><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow>
</m:mfrac>
