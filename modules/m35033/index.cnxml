<m:mfrac>
<m:mrow>
<m:mo>-</m:mo>
<m:mn>6</m:mn><m:mo>(</m:mo><m:mo>-</m:mo>
<m:mn>3</m:mn><m:mo>)</m:mo><m:mo>-</m:mo><m:mn>2</m:mn><m:mo>(</m:mo><m:mo>-</m:mo>
<m:mn>1</m:mn><m:mo>)</m:mo>
</m:mrow>
<m:mrow>
<m:mo>-</m:mo><m:mo>(</m:mo><m:mn>5</m:mn><m:mo>)</m:mo><m:mo>+</m:mo><m:mn>1</m:mn>
</m:mrow>
</m:mfrac>
