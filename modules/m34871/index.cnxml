</example>
<example id="eip-id1169113576079"><para id="id3614769">Find 


<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mroot><m:mtext>2187</m:mtext><m:mstyle fontsize="8pt"><m:mrow><m:mn>7</m:mn></m:mrow></m:mstyle></m:mroot></m:mrow></m:mstyle><m:mrow/></m:mrow></m:semantics></m:math>.</para>
        <table id="id3614794" summary="Instructions for calculator input.">
          <tgroup cols="3">
            <colspec colnum="1" colname="c1"/>
            <colspec colnum="2" colname="c2"/>
            <colspec colnum="3" colname="c3"/>
            <tbody>
              <row>
                <entry/>
                <entry/>
                <entry>Display Reads</entry>
              </row>
              <row>
                <entry>Type</entry>
