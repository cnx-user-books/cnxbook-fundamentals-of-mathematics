                            <m:msup>
                              <m:mi>y</m:mi>
                              <m:mstyle fontsize="8pt">
                                <m:mrow>
                                  <m:mi>x</m:mi>
                                </m:mrow>
                              </m:mstyle>
                            </m:msup>
                          </m:mrow>
                        </m:mstyle>
                        <m:mrow/>
                      </m:mrow>
                      <m:annotation encoding="StarMath 5.0"> size 12{y rSup { size 8{x} } } {}</m:annotation>
                    </m:semantics>
                  </m:math>
                </entry>
                <entry>2187</entry>
              </row>
              <row>
                <entry>Type</entry>
