                                  </m:mfrac>
</m:mtd>
                    </m:mtr>
                    <m:mtr>
<m:mtd/>
                      
<m:mtd> <m:mo stretchy="false">=</m:mo> </m:mtd>
<m:mtd columnalign="left">
                                 
                                    <m:mfrac>
                                      <m:mtext>15</m:mtext>
                                      <m:mn>6</m:mn>
                                    </m:mfrac>
                                    <m:mo stretchy="false">−</m:mo>
                                    <m:mfrac>
