                                  <m:mtext>16</m:mtext>
                                </m:mrow>
                                <m:mtext>80</m:mtext>
                              </m:mfrac>
                              <m:mo stretchy="false">+</m:mo>
                              <m:mfrac>
                                <m:mrow>
                                  <m:mn>1</m:mn>
                                  <m:mo stretchy="false">⋅</m:mo>
                                  <m:mn>5</m:mn>
                                </m:mrow>
                                <m:mtext>80</m:mtext>
                              </m:mfrac>
                            </m:mrow>
                          </m:mrow>
                          <m:mo stretchy="false">=</m:mo>
                          <m:mrow>
                            <m:mfrac>
                              <m:mtext>48</m:mtext>
                              <m:mtext>80</m:mtext>
                            </m:mfrac>
                            <m:mo stretchy="false">+</m:mo>
                            <m:mfrac>
                              <m:mn>5</m:mn>
                              <m:mtext>80</m:mtext>
                            </m:mfrac>
                          </m:mrow>
                        </m:mrow>
                        <m:mo stretchy="false">=</m:mo>
                        <m:mfrac>
                          <m:mrow>
                            <m:mtext>48</m:mtext>
                            <m:mo stretchy="false">+</m:mo>
                            <m:mn>5</m:mn>
                          </m:mrow>
                          <m:mtext>80</m:mtext>
                        </m:mfrac>
                      </m:mrow>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mfrac>
                        <m:mtext>53</m:mtext>
                        <m:mtext>80</m:mtext>
                      </m:mfrac>
                    </m:mrow>
                  </m:mrow>
                </m:mstyle>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ {  {3}  over  {5} } + {  {1}  over  {"16"} } = {  {3 cdot "16"}  over  {"80"} } + {  {1 cdot 5}  over  {"80"} } = {  {"48"}  over  {"80"} } + {  {5}  over  {"80"} } = {  {"48"+5}  over  {"80"} } = {  {"53"}  over  {"80"} } } {}</m:annotation>
            </m:semantics>
          </m:math>
        </para>
<newline/>
<para id="id1171866094992">Thus, 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mrow><m:mfrac><m:mn>3</m:mn><m:mn>5</m:mn></m:mfrac><m:mo stretchy="false">+</m:mo><m:mfrac><m:mn>9</m:mn><m:mtext>44</m:mtext></m:mfrac></m:mrow><m:mrow><m:mfenced open="(" close=")"><m:mrow><m:mfrac><m:mn>5</m:mn><m:mn>9</m:mn></m:mfrac><m:mo stretchy="false">−</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>4</m:mn></m:mfrac></m:mrow></m:mfenced><m:mo stretchy="false">=</m:mo><m:mfrac><m:mtext>53</m:mtext><m:mtext>80</m:mtext></m:mfrac></m:mrow></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ {  {3}  over  {5} } + {  {9}  over  {"44"} }  left ( {  {5}  over  {9} }  -  {  {1}  over  {4} }  right )= {  {"53"}  over  {"80"} } } {}</m:annotation></m:semantics></m:math></para>
