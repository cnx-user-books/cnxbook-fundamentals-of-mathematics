                              <m:mrow>
                                <m:mn>2</m:mn>
                              </m:mrow>
                            </m:mstyle>
                          </m:msup>
                          <m:mo stretchy="false">=</m:mo>
                          <m:mrow>
                            <m:mfrac>
                              <m:mn>3</m:mn>
                              <m:mn>4</m:mn>
                            </m:mfrac>
                            <m:mo stretchy="false">⋅</m:mo>
                            <m:mfrac>
                              <m:mn>3</m:mn>
                              <m:mn>4</m:mn>
                            </m:mfrac>
                          </m:mrow>
                        </m:mrow>
                        <m:mo stretchy="false">=</m:mo>
                        <m:mfrac>
                          <m:mrow>
                            <m:mn>3</m:mn>
                            <m:mo stretchy="false">⋅</m:mo>
                            <m:mn>3</m:mn>
                          </m:mrow>
                          <m:mrow>
                            <m:mn>4</m:mn>
                            <m:mo stretchy="false">⋅</m:mo>
                            <m:mn>4</m:mn>
                          </m:mrow>
                        </m:mfrac>
                      </m:mrow>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mfrac>
                        <m:mn>9</m:mn>
                        <m:mtext>16</m:mtext>
                      </m:mfrac>
                    </m:mrow>
                  </m:mrow>
                </m:mstyle>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ left ( {  {3}  over  {4} }  right ) rSup { size 8{2} } = {  {3}  over  {4} }  cdot  {  {3}  over  {4} } = {  {3 cdot 3}  over  {4 cdot 4} } = {  {9}  over  {"16"} } } {}</m:annotation>
            </m:semantics>
          </m:math>
        </para><para id="id1171861426631">Now we have</para>
        <para id="id1171867919518">
          <m:math>
            <m:semantics>
              <m:mrow>
                <m:mstyle fontsize="12pt">
                  <m:mrow>
                    <m:mrow>
                      <m:mrow>
                        <m:mfrac>
                          <m:mn>9</m:mn>
                          <m:mtext>16</m:mtext>
