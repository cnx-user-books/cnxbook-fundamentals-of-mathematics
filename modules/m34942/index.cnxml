                        <m:mn>2</m:mn>
                      </m:mrow>
                      <m:mrow>
                        <m:mrow>
                          <m:mfrac>
                            <m:mn>7</m:mn>
                            <m:mn>8</m:mn>
                          </m:mfrac>
                          <m:mo stretchy="false">+</m:mo>
                          <m:mfrac>
                            <m:mrow>
                              <m:mn>5</m:mn>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mn>1</m:mn>
                            </m:mrow>
                            <m:mrow>
                              <m:mn>1</m:mn>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mn>7</m:mn>
                            </m:mrow>
                          </m:mfrac>
                        </m:mrow>
                        <m:mo stretchy="false">=</m:mo>
                        <m:mn>2</m:mn>
                      </m:mrow>
                      <m:mrow>
                        <m:mfrac>
                          <m:mn>7</m:mn>
                          <m:mn>8</m:mn>
                        </m:mfrac>
                        <m:mo stretchy="false">+</m:mo>
                        <m:mfrac>
                          <m:mn>5</m:mn>
                          <m:mn>7</m:mn>
                        </m:mfrac>
                      </m:mrow>
                    </m:mrow>
                  </m:mrow>
                </m:mstyle>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{2 {  {7}  over  {8} } + {  {5}  over  { {6}  cSub { size 8{1} } } }  cdot  {  { {6}  cSup { size 8{1} } }  over  {7} } =2 {  {7}  over  {8} } + {  {5 cdot 1}  over  {1 cdot 7} } =2 {  {7}  over  {8} } + {  {5}  over  {7} } } {}</m:annotation>
            </m:semantics>
          </m:math>
        </para>
</item>
<item>Now perform the addition.<newline/>
        <para id="id1171860347628"><m:math>
                       
                  <m:mtable>
