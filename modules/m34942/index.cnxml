<m:mn>2</m:mn>
<m:mfrac>
<m:mn>1</m:mn>
<m:mn>8</m:mn>
</m:mfrac>
</m:mrow>
</m:mfenced>
<m:mo>=</m:mo>
<m:mn>7</m:mn>
<m:mfrac>
<m:mn>69</m:mn>
<m:mn>80</m:mn>
</m:mfrac>
</m:mrow>
</m:math></para>
