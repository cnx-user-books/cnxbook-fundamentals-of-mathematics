              <m:annotation encoding="StarMath 5.0"> size 12{2 {  {7}  over  {8} } + sqrt { {  {"25"}  over  {"36"} } }  div  left (2 {  {1}  over  {2} }  - 1 {  {1}  over  {3} }  right )} {}</m:annotation>
            </m:semantics>
          </m:math>
        </para>

<list id="eip-id1169232387238" mark-prefix="(" mark-suffix=")" list-type="enumerated" number-style="lower-alpha"><item>Begin by operating inside the parentheses.<newline/>
        <para id="eip-935"><m:math>
           
           
                  <m:mtable>
                    <m:mtr>
                      <m:mtd>
                              <m:mn>2</m:mn>
                              
                                <m:mfrac>
                                  <m:mn>1</m:mn>
                                  <m:mn>2</m:mn>
                                </m:mfrac>
                                <m:mo stretchy="false">−</m:mo>
                                <m:mn>1</m:mn>
                              
                                                                <m:mfrac>
                                    <m:mn>1</m:mn>
                                    <m:mn>3</m:mn>
                                  </m:mfrac>
</m:mtd>
<m:mtd>
                                  <m:mo stretchy="false">=</m:mo>
    </m:mtd>
<m:mtd columnalign="left">                             
                                    <m:mfrac>
                                      <m:mrow>
                                          <m:mn>2</m:mn>
                                          <m:mo stretchy="false">⋅</m:mo>
                                          <m:mn>2</m:mn>
                                        
                                        <m:mo stretchy="false">+</m:mo>
                                        <m:mn>1</m:mn>
                                      </m:mrow>
                                      <m:mn>2</m:mn>
                                    </m:mfrac>
                                    <m:mo stretchy="false">−</m:mo>
                                    <m:mfrac>
                                      <m:mrow> 
                                          <m:mn>1</m:mn>
                                          <m:mo stretchy="false">⋅</m:mo>
                                          <m:mn>3</m:mn>
                                        
                                        <m:mo stretchy="false">+</m:mo>
                                        <m:mn>1</m:mn>
                                      </m:mrow>
                                      <m:mn>3</m:mn>
                                    </m:mfrac>
                                <m:mo stretchy="false">=</m:mo>
                                
                                  <m:mfrac>
                                    <m:mn>5</m:mn>
                                    <m:mn>2</m:mn>
                                  </m:mfrac>
                                  <m:mo stretchy="false">−</m:mo>
                                  <m:mfrac>
                                    <m:mn>4</m:mn>
