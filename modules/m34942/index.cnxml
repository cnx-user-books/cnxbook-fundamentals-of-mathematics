        <para id="id1171868490839">Thus, 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mn>2</m:mn><m:mrow><m:mrow><m:mfrac><m:mn>7</m:mn><m:mn>8</m:mn></m:mfrac><m:mo stretchy="false">+</m:mo><m:mrow><m:msqrt><m:mfrac><m:mtext>25</m:mtext><m:mtext>36</m:mtext></m:mfrac></m:msqrt><m:mo stretchy="false">÷</m:mo><m:mfenced open="(" close=")"><m:mrow><m:mn>2</m:mn><m:mrow><m:mfrac><m:mn>1</m:mn><m:mn>2</m:mn></m:mfrac><m:mo stretchy="false">−</m:mo><m:mn>1</m:mn></m:mrow><m:mfrac><m:mn>1</m:mn><m:mn>3</m:mn></m:mfrac></m:mrow></m:mfenced></m:mrow></m:mrow><m:mo stretchy="false">=</m:mo><m:mn>3</m:mn></m:mrow><m:mfrac><m:mtext>33</m:mtext><m:mtext>56</m:mtext></m:mfrac></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{2 {  {7}  over  {8} } + sqrt { {  {"25"}  over  {"36"} } }  div  left (2 {  {1}  over  {2} }  - 1 {  {1}  over  {3} }  right )=3 {  {"33"}  over  {"56"} } } {}</m:annotation></m:semantics></m:math></para>
