                              <m:mn>1</m:mn>
                            </m:mrow>
                            <m:mrow>
                              <m:mrow>
                                <m:mn>2</m:mn>
                                <m:mo stretchy="false">⋅</m:mo>
                                <m:mn>1</m:mn>
                              </m:mrow>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mtext>40</m:mtext>
                            </m:mrow>
                          </m:mfrac>
                        </m:mrow>
                      </m:mrow>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mrow>
                        <m:mn>8</m:mn>
                        <m:mo stretchy="false">−</m:mo>
                        <m:mfrac>
                          <m:mtext>11</m:mtext>
                          <m:mtext>80</m:mtext>
                        </m:mfrac>
                      </m:mrow>
                    </m:mrow>
                  </m:mrow>
                </m:mstyle>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{8 -  {  { {"15"}  cSup { size 8{1} } }  over  { {"426"}  cSub { size 8{2} } } }  cdot  {  {"11"}  over  { {"15"}  cSub { size 8{1} } } }  cdot  {  { {"213"}  cSup { size 8{1} } }  over  {"40"} } =8 -  {  {1 cdot "11" cdot 1}  over  {2 cdot 1 cdot "40"} } =8 -  {  {"11"}  over  {"80"} } } {}</m:annotation>
            </m:semantics>
          </m:math>
        </para>
</item>
<item>
Now subtract.<newline/>
        
        <para id="eip-113"><m:math>
            <m:semantics>
              <m:mrow>
                <m:mstyle fontsize="12pt">
                  <m:mrow>
                    <m:mrow>
                      <m:mrow>
                        <m:mrow>
                          <m:mrow>
                            <m:mrow>
                              <m:mrow>
                                <m:mn>8</m:mn>
                                <m:mo stretchy="false">−</m:mo>
                                <m:mfrac>
                                  <m:mtext>11</m:mtext>
                                  <m:mtext>80</m:mtext>
                                </m:mfrac>
                              </m:mrow>
                              <m:mo stretchy="false">=</m:mo>
                              <m:mrow>
                                <m:mfrac>
                                  <m:mrow>
                                    <m:mtext>80</m:mtext>
                                    <m:mo stretchy="false">⋅</m:mo>
                                    <m:mn>8</m:mn>
                                  </m:mrow>
                                  <m:mtext>80</m:mtext>
                                </m:mfrac>
                                <m:mo stretchy="false">−</m:mo>
                                <m:mfrac>
                                  <m:mtext>11</m:mtext>
                                  <m:mtext>80</m:mtext>
                                </m:mfrac>
                              </m:mrow>
                            </m:mrow>
                            <m:mo stretchy="false">=</m:mo>
                            <m:mrow>
                              <m:mfrac>
                                <m:mtext>640</m:mtext>
                                <m:mtext>80</m:mtext>
                              </m:mfrac>
                              <m:mo stretchy="false">−</m:mo>
                              <m:mfrac>
                                <m:mtext>11</m:mtext>
                                <m:mtext>80</m:mtext>
                              </m:mfrac>
                            </m:mrow>
                          </m:mrow>
                          <m:mo stretchy="false">=</m:mo>
                          <m:mfrac>
                            <m:mrow>
                              <m:mtext>640</m:mtext>
                              <m:mo stretchy="false">−</m:mo>
                              <m:mtext>11</m:mtext>
                            </m:mrow>
                            <m:mtext>80</m:mtext>
                          </m:mfrac>
                        </m:mrow>
                        <m:mo stretchy="false">=</m:mo>
                        <m:mfrac>
                          <m:mtext>629</m:mtext>
                          <m:mtext>80</m:mtext>
                        </m:mfrac>
                      </m:mrow>
<m:mspace width="4px"/>
                      <m:mtext> or </m:mtext>
<m:mspace width="4px"/>
                      <m:mn>7</m:mn>
                      <m:mfrac>
                        <m:mtext>69</m:mtext>
                        <m:mtext>80</m:mtext>
                      </m:mfrac>
                    </m:mrow>
                  </m:mrow>
                </m:mstyle>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{8 -  {  {"11"}  over  {"80"} } = {  {"80" cdot 8}  over  {"80"} }  -  {  {"11"}  over  {"80"} } = {  {"640"}  over  {"80"} }  -  {  {"11"}  over  {"80"} } = {  {"640" - "11"}  over  {"80"} } = {  {"629"}  over  {"80"} } " or "7 {  {"69"}  over  {"80"} } } {}</m:annotation>
            </m:semantics>
          </m:math>
        </para><newline/>
        <para id="eip-330">Thus, <m:math>
<m:mrow>
<m:mn>8</m:mn>
<m:mo>-</m:mo>
<m:mfrac>
<m:mn>15</m:mn>
<m:mn>426</m:mn>
</m:mfrac>
<m:mfenced open="(" close=")">
<m:mrow>
