    <section id="id1170053966178"><title>Practice Set A</title><para id="id1170040269097">Perform the following operations.</para>
      <exercise id="id1170061130819">
        <problem id="oo-id1170039011056">
          <para id="id1170059910482">
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mrow><m:mfrac><m:mn>3</m:mn><m:mn>5</m:mn></m:mfrac><m:mo stretchy="false">+</m:mo><m:mn>1</m:mn></m:mrow><m:mtext>.</m:mtext><m:mn>6</m:mn></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ {  {3}  over  {5} } +1 "." 6} {}</m:annotation></m:semantics></m:math>   </para>
        </problem>
<solution id="eip-id1166491912890"><para id="eip-id1166487784869">2.2 or 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mn>2</m:mn><m:mfrac><m:mn>1</m:mn><m:mn>5</m:mn></m:mfrac></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{2 {  {1}  over  {5} } } {}</m:annotation></m:semantics></m:math></para></solution>
      </exercise>
      <exercise id="id1170057759623">
        <problem id="oo-id1170039076364">
          <para id="id1170041525842">
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mn>8</m:mn><m:mtext>.</m:mtext><m:mrow><m:mtext>91</m:mtext><m:mo stretchy="false">+</m:mo><m:mrow><m:mfrac><m:mn>1</m:mn><m:mn>5</m:mn></m:mfrac><m:mo stretchy="false">⋅</m:mo><m:mn>1</m:mn></m:mrow></m:mrow><m:mtext>.</m:mtext><m:mn>6</m:mn></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{8 "." "91"+ {  {1}  over  {5} }  cdot 1 "." 6} {}</m:annotation></m:semantics></m:math>  </para>
        </problem>
<solution id="eip-id1166487953580"><para id="eip-id1166498749255">9.23</para></solution>
      </exercise>
      <exercise id="id1170056221282">
