                  <m:mtr>
                 <m:mtd> 
                            <m:mfrac>
                              <m:mn>5</m:mn>
                              <m:mtext>13</m:mtext>
                            </m:mfrac>
                           
                              <m:mfenced open="(" close=")"><m:mrow>
                               
                                  <m:mfrac>
                                    <m:mn>4</m:mn>
                                    <m:mn>5</m:mn>
                                  </m:mfrac>
                                  <m:mo stretchy="false">−</m:mo>
                                  <m:mfrac>
                                    <m:mtext>28</m:mtext>
                                    <m:mtext>100</m:mtext>
                                  </m:mfrac>
                  </m:mrow>              
                              </m:mfenced>
</m:mtd>
<m:mtd>
                              <m:mo stretchy="false">=</m:mo>
</m:mtd>
<m:mtd columnalign="left">
                              <m:mfrac>
                                <m:mn>5</m:mn>
                                <m:mtext>13</m:mtext>
                              </m:mfrac>
                          
                            <m:mfenced open="(" close=")">
                              <m:mrow>
                                <m:mfrac>
                                  <m:mn>4</m:mn>
                                  <m:mn>5</m:mn>
                                </m:mfrac>
                                <m:mo stretchy="false">−</m:mo>
                                <m:mfrac>
                                  <m:mn>7</m:mn>
                                  <m:mtext>25</m:mtext>
                                </m:mfrac>
                              </m:mrow>
                            </m:mfenced>
                        </m:mtd>
                  </m:mtr>
                  <m:mtr>
                   <m:mtd/>
                    <m:mtd> <m:mo stretchy="false">=</m:mo></m:mtd>
<m:mtd columnalign="left">
                              <m:mfrac>
                                <m:mn>5</m:mn>
                                <m:mtext>13</m:mtext>
                              </m:mfrac>
                           
                            <m:mfenced open="(" close=")"><m:mrow>
                              
                                <m:mfrac>
                                  <m:mtext>20</m:mtext>
                                  <m:mtext>25</m:mtext>
                                </m:mfrac>
                                <m:mo stretchy="false">−</m:mo>
                                <m:mfrac>
                                  <m:mn>7</m:mn>
                                  <m:mtext>25</m:mtext>
                                </m:mfrac>
                              
</m:mrow>                  
          </m:mfenced>
</m:mtd>                         
                  </m:mtr>
                  <m:mtr>
                    <m:mtd/>

                         <m:mtd>  <m:mo stretchy="false">=</m:mo></m:mtd>
<m:mtd columnalign="left">
                           
                              <m:mfrac>
                                <m:mover>
                <m:menclose notation="updiagonalstrike"><m:mn>5</m:mn></m:menclose>
                                 
                                   
                                      <m:mn>1</m:mn>
                                                                 
                                </m:mover>
                                <m:munder>
                                  <m:mrow>
                                    <m:menclose notation="updiagonalstrike"><m:mn>13</m:mn></m:menclose>
                                   
                                  </m:mrow>
                                  
                                    <m:mrow>
                                      <m:mn>1</m:mn>
                                    </m:mrow>
                                  
                                </m:munder>
                              </m:mfrac>
                              <m:mo stretchy="false">⋅</m:mo>
                              <m:mfrac>
                                <m:mover>
                                  <m:mrow>
              <m:menclose notation="updiagonalstrike">   <m:mn>13</m:mn></m:menclose>
                                    
                                  </m:mrow>
                                  <m:mstyle fontsize="8pt">
                                    <m:mrow>
                                      <m:mn>1</m:mn>
                                    </m:mrow>
                                  </m:mstyle>
                                </m:mover>
                                <m:munder>
                                  <m:mrow>
                  <m:menclose notation="updiagonalstrike"> <m:mn>25</m:mn></m:menclose>
                                    
                                  </m:mrow>
                                  <m:mstyle fontsize="8pt">
                                    <m:mrow>
                                      <m:mn>5</m:mn>
                                    </m:mrow>
                                  </m:mstyle>
                                </m:munder>
                              </m:mfrac>
                           </m:mtd>
                  </m:mtr>
                  <m:mtr>
                 <m:mtd/>
<m:mtd><m:mo>=</m:mo></m:mtd>
<m:mtd columnalign="left">
                      <m:mfrac>
                        <m:mn>1</m:mn>
                        <m:mn>5</m:mn>
                      </m:mfrac>
                     </m:mtd>
                  </m:mtr>
                </m:mtable>
              
        </m:math>
      </para>
</example>
