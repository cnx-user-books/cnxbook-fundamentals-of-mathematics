</exercise>
      <exercise id="id1170286443816"><problem id="oo-id1170301791051">
          <para id="id1170298605700">(<link document="m35012"/>) 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mrow><m:mrow><m:mtext>58 </m:mtext><m:mo stretchy="false">+</m:mo><m:mtext> 91 </m:mtext></m:mrow><m:mo stretchy="false">+</m:mo><m:mtext> 61 </m:mtext></m:mrow><m:mo stretchy="false">+</m:mo><m:mtext> 88</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"58 "+" 91 "+" 61 "+" 88"} {}</m:annotation></m:semantics></m:math></para>
        </problem>
      <solution id="eip-id1164970239995"><para id="eip-id2058781"> <m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mn>2</m:mn><m:mrow><m:mfenced open="(" close=")"><m:mtext>60</m:mtext></m:mfenced><m:mo stretchy="false">+</m:mo><m:mn>2</m:mn></m:mrow><m:mrow><m:mfenced open="(" close=")"><m:mtext>90</m:mtext></m:mfenced><m:mo stretchy="false">=</m:mo><m:mtext>300</m:mtext></m:mrow></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{2 left ("60" right )+2 left ("90" right )="300"} {}</m:annotation></m:semantics></m:math> (298)</para></solution>
</exercise>
      <exercise id="id1170278271525"><problem id="oo-id1170287382815">
          <para id="id1170278917909">(<link document="m35012"/>) 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mrow><m:mrow><m:mtext>43 </m:mtext><m:mo stretchy="false">+</m:mo><m:mtext> 39 </m:mtext></m:mrow><m:mo stretchy="false">+</m:mo><m:mtext> 89 </m:mtext></m:mrow><m:mo stretchy="false">+</m:mo><m:mtext> 92</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"43 "+" 39 "+" 89 "+" 92"} {}</m:annotation></m:semantics></m:math></para>
        </problem>
      <solution id="eip-id1164920600441"><para id="eip-id1164921099257"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mn>2</m:mn><m:mo stretchy="false">(</m:mo><m:mtext>40</m:mtext><m:mrow><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">+</m:mo><m:mn>2</m:mn></m:mrow><m:mo stretchy="false">(</m:mo><m:mtext>90</m:mtext><m:mrow><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">=</m:mo><m:mtext>260</m:mtext></m:mrow></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{2 \( "40" \) +2 \( "90" \) ="260"} {}</m:annotation></m:semantics></m:math> (263)</para></solution></exercise>
      <exercise id="id1170278835712"><problem id="oo-id1170283138493">
          <para id="id1170293189653">(<link document="m35012"/>) 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mrow><m:mrow><m:mtext>81 </m:mtext><m:mo stretchy="false">+</m:mo><m:mtext> 78 </m:mtext></m:mrow><m:mo stretchy="false">+</m:mo><m:mtext> 27 </m:mtext></m:mrow><m:mo stretchy="false">+</m:mo><m:mtext> 79</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"81 "+" 78 "+" 27 "+" 79"} {}</m:annotation></m:semantics></m:math></para>
        </problem>
      <solution id="eip-id1166852867295"><para id="eip-id1166850446449"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mrow><m:mtext>30</m:mtext><m:mo stretchy="false">+</m:mo><m:mn>3</m:mn></m:mrow><m:mo stretchy="false">(</m:mo><m:mtext>80</m:mtext><m:mrow><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">=</m:mo><m:mtext>270</m:mtext></m:mrow></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"30"+3 \( "80" \) ="270"} {}</m:annotation></m:semantics></m:math> (265) </para></solution>
</exercise>
      <exercise id="id1170278918006"><problem id="oo-id5261820">
          <para id="id1170300513247">(<link document="m35012"/>) 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mrow><m:mrow><m:mrow><m:mtext>804 </m:mtext><m:mo stretchy="false">+</m:mo><m:mtext> 612 </m:mtext></m:mrow><m:mo stretchy="false">+</m:mo><m:mtext> 801 </m:mtext></m:mrow><m:mo stretchy="false">+</m:mo><m:mtext> 795</m:mtext></m:mrow><m:mo stretchy="false">+</m:mo><m:mtext> 606</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"804 "+" 612 "+" 801 "+" 795"+" 606"} {}</m:annotation></m:semantics></m:math></para>
        </problem>
<solution id="eip-id1168887075464"><para id="eip-id1168877421440"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mn>3</m:mn><m:mo stretchy="false">(</m:mo><m:mtext>800</m:mtext><m:mrow><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">+</m:mo><m:mn>2</m:mn></m:mrow><m:mo stretchy="false">(</m:mo><m:mtext>600</m:mtext><m:mrow><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">=</m:mo><m:mn>3,</m:mn></m:mrow><m:mtext>600</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{3 \( "800" \) +2 \( "600" \) =3,"600"} {}</m:annotation></m:semantics></m:math> (3,618) </para></solution>

      </exercise>
      <para id="id1170281354492">For problems 17-21, use the distributive property to obtain the exact result.</para>
      <exercise id="id1170280035109"><problem id="oo-id1170282535358">
          <para id="id1170278293283">(<link document="m35013"/>) 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>25</m:mtext><m:mo stretchy="false">⋅</m:mo><m:mtext> 14</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"25" cdot " 14"} {}</m:annotation></m:semantics></m:math></para>
        </problem>
      <solution id="eip-id1171845134948"><para id="eip-id1171853658151"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>25</m:mtext><m:mo stretchy="false">(</m:mo><m:mrow><m:mtext>10</m:mtext><m:mo stretchy="false">+</m:mo><m:mn>4</m:mn></m:mrow><m:mrow><m:mrow><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">=</m:mo><m:mrow><m:mtext>250</m:mtext><m:mo stretchy="false">+</m:mo><m:mtext>100</m:mtext></m:mrow></m:mrow><m:mo stretchy="false">=</m:mo><m:mtext>350</m:mtext></m:mrow></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"25" \( "10"+4 \) ="250"+"100"="350"} {}</m:annotation></m:semantics></m:math> </para></solution>
</exercise>
      <exercise id="id1170277480341"><problem id="oo-id1170277483983">
          <para id="id1170290204101">(<link document="m35013"/>) 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>15 </m:mtext><m:mo stretchy="false">⋅</m:mo><m:mtext> 83</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"15 " cdot " 83"} {}</m:annotation></m:semantics></m:math></para>
        </problem>
      <solution id="eip-id1168875089393"><para id="eip-id1168888425428"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>15</m:mtext><m:mo stretchy="false">(</m:mo><m:mrow><m:mtext>80</m:mtext><m:mo stretchy="false">+</m:mo><m:mn>3</m:mn></m:mrow><m:mrow><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">=</m:mo><m:mn>1,</m:mn></m:mrow><m:mrow><m:mrow><m:mtext>200</m:mtext><m:mo stretchy="false">+</m:mo><m:mtext>45</m:mtext></m:mrow><m:mo stretchy="false">=</m:mo><m:mn>1,</m:mn></m:mrow><m:mtext>245</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"15" \( "80"+3 \) =1,"200"+"45"=1,"245"} {}</m:annotation></m:semantics></m:math> </para></solution>
</exercise>
      <exercise id="id1170280400378"><problem id="oo-id1170295386942">
          <para id="id1170278813345">(<link document="m35013"/>) 
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>65 </m:mtext><m:mo stretchy="false">⋅</m:mo><m:mtext> 98</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"65 " cdot " 98"} {}</m:annotation></m:semantics></m:math></para>
        </problem>
      <solution id="eip-id1166676233449"><para id="eip-id2041687"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>65</m:mtext><m:mo stretchy="false">(</m:mo><m:mrow><m:mtext>100</m:mtext><m:mo stretchy="false">−</m:mo><m:mn>2</m:mn></m:mrow><m:mrow><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">=</m:mo><m:mn>6,</m:mn></m:mrow><m:mrow><m:mrow><m:mtext>500</m:mtext><m:mo stretchy="false">−</m:mo><m:mtext>130</m:mtext></m:mrow><m:mo stretchy="false">=</m:mo><m:mn>6,</m:mn></m:mrow><m:mtext>370</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"65" \( "100" - 2 \) =6,"500" - "130"=6,"370"} {}</m:annotation></m:semantics></m:math> </para></solution>
