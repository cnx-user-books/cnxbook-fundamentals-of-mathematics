        </exercise>
        <exercise id="id1167555483089">
          <problem id="oo-id1167553664238">
            <para id="id1167555587222">
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>32,051</m:mtext><m:mtext>.</m:mtext><m:mrow><m:mtext>3585</m:mtext><m:mo stretchy="false">÷</m:mo><m:mtext>23</m:mtext></m:mrow><m:mi>,</m:mi><m:mtext>006</m:mtext><m:mtext>.</m:mtext><m:mtext>9999</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"32,051" "." "3585" div "23","006" "." "9999"} {}</m:annotation></m:semantics></m:math></para>
          </problem>
        </exercise>
        <exercise id="id1167554368871">
          <problem id="oo-id1167554709531">
            <para id="id5376057">
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mrow><m:mtext>100</m:mtext><m:mo stretchy="false">⋅</m:mo><m:mn>1,</m:mn></m:mrow><m:mtext>816</m:mtext><m:mtext>.</m:mtext><m:mtext>001</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"100" cdot 1,"816" "." "001"} {}</m:annotation></m:semantics></m:math></para>
