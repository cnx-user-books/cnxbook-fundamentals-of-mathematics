        </exercise>
        <exercise id="id1167556484020">
          <problem id="oo-id6158424">
            <para id="id1167553677256">
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mtext>10</m:mtext><m:mtext>.</m:mtext><m:mrow><m:mtext>000</m:mtext><m:mo stretchy="false">⋅</m:mo><m:mn>0</m:mn></m:mrow><m:mtext>.</m:mtext><m:mtext>14</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{"10" "." "000" cdot 0 "." "14"} {}</m:annotation></m:semantics></m:math>    </para>
          </problem>
