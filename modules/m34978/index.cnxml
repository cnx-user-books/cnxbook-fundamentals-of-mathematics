        </exercise>
        <exercise id="id7749124">
          <problem id="oo-id1167553742124">
            <para id="id1167557362070">
<m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:mn>0</m:mn><m:mtext>.</m:mtext><m:mrow><m:mtext>135888</m:mtext><m:mo stretchy="false">÷</m:mo><m:mtext>16</m:mtext></m:mrow><m:mtext>.</m:mtext><m:mtext>986</m:mtext></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{0 "." "135888" div "16" "." "986"} {}</m:annotation></m:semantics></m:math></para>
