            <colspec colnum="3" colname="c3"/>
            <tbody>
              <row>
                <entry/>
                <entry/>
                <entry>Display Reads</entry>
              </row>
              <row>
                <entry>Type</entry>
                <entry>.5696376</entry>
                <entry>0.5696376</entry>
              </row>
              <row>
                <entry>Press</entry>
                <entry>÷</entry>
                <entry>0.5696376</entry>
              </row>
              <row>
                <entry>Type</entry>
                <entry>.00123</entry>
                <entry>0.00123</entry>
              </row>
              <row>
                <entry>Press</entry>
                <entry>=</entry>
                <entry>463.12</entry>
              </row>
            </tbody>
          
</tgroup>
