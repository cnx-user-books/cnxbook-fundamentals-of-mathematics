            <para id="id1172466056791">3 and 4</para>
          </problem>
        <solution id="eip-id1165624130848"><para id="eip-id1165629583799">12, 24, 36, 48, 60</para></solution></exercise>
        <exercise id="id1172456462699"><problem id="oo-id1172462525794">
            <para id="id1172456579207">2 and 5</para>
          </problem>
